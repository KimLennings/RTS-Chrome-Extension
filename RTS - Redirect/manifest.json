{
  "name": "RTS - Redirect",
  "version": "1.0",
  "manifest_version": 2,
  "content_scripts": [
    {
      "matches": ["https://webshop.infrontitpartner.se/order_history?showAll=true&freetext=*"],
      "js": ["content.js"]
      
    }
